    {
      "path": "dist/jm-product-cart-hoc.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "@types/react-test-renderer": "^18.0.0",
    "@babel/preset-react": "^7.22.5",
